{
    "name": "SAIS Dark Theme",
    "colors": {
        "editor.background": "#1E1E1E",
        "editor.foreground": "#D4D4D4"
    },
    "tokenColors": [
        {
            "scope": "constant.datetime.sais",
            "settings": {
                "foreground": "#FF5722"
            }
        },
        {
            "scope": "constant.time.sais",
            "settings": {
                "foreground": "#FF5722"
            }
        },
        {
            "scope": "constant.date.sais",
            "settings": {
                "foreground": "#FF5722"
            }
        },
        {
            "scope": "constant.numeric.sais",
            "settings": {
                "foreground": "#8BC34A"
            }
        },
        {
            "scope": "constant.url.sais",
            "settings": {
                "foreground": "#89DDFF"
            }
        },
        {
            "scope": "keyword.control.sais",
            "settings": {
                "foreground": "#f844ae",
