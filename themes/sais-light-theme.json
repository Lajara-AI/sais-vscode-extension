                "foreground": "#4CAF50"
            }
        },
        {
            "scope": "variable.other.sais",
            "settings": {
                "foreground": "#000000"
            }
        },
        {
            "scope": "comment.block.sais",
            "settings": {
                "foreground": "#757575"
            }
        },
        {
            "scope": "comment.line.double-slash.sais",
            "settings": {
                "foreground": "#757575"
            }
        }
    ]
}
