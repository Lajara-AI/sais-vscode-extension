{
    "name": "SAIS Light Theme",
    "colors": {
        "editor.background": "#FFFFFF",
        "editor.foreground": "#333333"
    },
    "tokenColors": [
        {
            "scope": "constant.datetime.sais",
            "settings": {
                "foreground": "#FF5722"
            }
        },
        {
            "scope": "constant.time.sais",
            "settings": {
                "foreground": "#FF5722"
            }
        },
        {
            "scope": "constant.date.sais",
            "settings": {
                "foreground": "#FF5722"
            }
        },
        {
            "scope": "constant.numeric.sais",
            "settings": {
                "foreground": "#388E3C"
            }
        },
        {
            "scope": "constant.url.sais",
            "settings": {
                "foreground": "#1976D2"
            }
        },
        {
            "scope": "keyword.control.sais",
            "settings": {
                "foreground": "#D81B60",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "keyword.other.sais",
            "settings": {
                "foreground": "#FFA000",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "keyword.type.sais",
            "settings": {
                "foreground": "#303F9F",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "constant.language.sais",
            "settings": {
                "foreground": "#E64A19"
            }
        },
        {
            "scope": "string.quoted.double.sais",
